{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "name": "Order"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "orderId": {
      "type": "uid"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "products": {
      "collection": "product"
    },
    "cart": {
      "via": "order",
      "model": "cart"
    }
  }
}
